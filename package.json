{
  "name": "beginner-blocs",
  "version": "1.0.0",
  "description": "An NFT Marketplace designed by beginners for beginners",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "lint": "eslint \"**/*.{ts,js}\" --cache --fix",
    "format": "prettier --write \"**/*.{ts,js,json,graphql,gql,yaml,yml,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,graphql,gql,yaml,yml,md}\""
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connie-ho/beginner-blocs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/connie-ho/beginner-blocs/issues"
  },
  "homepage": "https://github.com/connie-ho/beginner-blocs#readme",
  "dependencies": {
    "dotenv": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1"
  }
}
