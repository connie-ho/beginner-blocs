{
  "name": "beginner-blocs",
  "version": "1.0.0",
  "description": "An NFT Marketplace designed by beginners for beginners",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "lint": "eslint \"**/*.{js}\" --cache --fix",
    "format": "prettier --write \"**/*.{js,json,yaml,yml,md,sol}\""
  },
  "lint-staged": {
    "*.{ts,js,sol}": [
      "npm run format",
      "npm run lint",
      "git add ."
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,json,yaml,yml,md,sol}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connie-ho/beginner-blocs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/connie-ho/beginner-blocs/issues"
  },
  "homepage": "https://github.com/connie-ho/beginner-blocs#readme",
  "dependencies": {
    "dotenv": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1"
  }
}
