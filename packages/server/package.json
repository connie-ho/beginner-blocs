{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean transpile",
    "start": "node ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build start",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "lint": "eslint --cache --fix \"**/*.js\""
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "axios": "^0.26.0",
    "babel-polyfill": "^6.26.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.9",
    "retry-axios": "^2.6.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "nodemon": "^2.0.15"
  }
}
